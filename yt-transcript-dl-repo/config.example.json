{
  "name": "yt-transcript-mcp-server",
  "version": "1.0.0",
  "logLevel": "info",
  "transports": {
    "stdio": {
      "enabled": true
    },
    "sse": {
      "enabled": true,
      "port": 3456,
      "host": "0.0.0.0",
      "cors": {
        "origin": "*",
        "credentials": false
      },
      "keepAliveInterval": 30000,
      "maxClients": 100
    },
    "http": {
      "enabled": true,
      "port": 3457,
      "host": "0.0.0.0",
      "cors": {
        "origin": "*",
        "credentials": false
      },
      "maxBodySize": "10mb",
      "timeout": 30000,
      "rateLimit": {
        "windowMs": 60000,
        "max": 100
      }
    }
  }
}