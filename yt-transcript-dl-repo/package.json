{
  "name": "youtube-transcript-extractor",
  "version": "1.0.0",
  "description": "YouTube transcript extraction service with MCP server support",
  "main": "dist/index.js",
  "type": "module",
  "bin": {
    "yt-transcript": "./dist/cli.js",
    "youtube-transcript-mcp": "./dist/server/index.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsx --watch src/server/index.ts",
    "start": "node dist/server/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint . --ext .ts,.tsx",
    "format": "prettier --write \"src/**/*.{ts,tsx,json,md}\""
  },
  "keywords": [
    "youtube",
    "transcript",
    "mcp",
    "api"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.0.0",
    "axios": "^1.6.0",
    "cheerio": "^1.0.0-rc.12",
    "commander": "^11.0.0",
    "express": "^4.18.2",
    "lru-cache": "^10.0.0",
    "p-limit": "^5.0.0",
    "winston": "^3.11.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.0",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "jest": "^29.5.0",
    "prettier": "^3.0.0",
    "ts-jest": "^29.1.0",
    "tsx": "^4.0.0",
    "typescript": "^5.0.0"
  }
}